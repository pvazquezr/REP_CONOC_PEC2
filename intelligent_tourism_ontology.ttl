@prefix : <http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix intelligent_tourism_ontology: <http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#> .
@base <http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology/> .

<https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl> rdf:type owl:Ontology ;
                                                                <http://purl.org/dc/elements/1.1/created> "\"2025-04-27\"^^xsd:date" ;
                                                                <http://purl.org/dc/elements/1.1/creator> "Pablo Vázquez Rodríguez" ;
                                                                <http://purl.org/dc/elements/1.1/description> "In this ontology we model the domain of intelligent tourism, gathering information from different sources like events, hotels, attractions and restaurants in main touristic cities. The main goal is to provide recommendation systems that provide travelers useful informacion from a semantic structure able to combine geo data along user review values, events or activities." ;
                                                                <http://purl.org/dc/elements/1.1/license> "https://creativecommons.org/licenses/by/4.0/" ;
                                                                <http://purl.org/dc/elements/1.1/title> "Recommendations and intelligent tourism ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/created
<http://purl.org/dc/elements/1.1/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/license
<http://purl.org/dc/elements/1.1/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdf:type owl:FunctionalProperty ,
                    owl:AsymmetricProperty ;
           rdfs:domain :TourismItem ;
           rdfs:range :City ;
           rdfs:comment "Indicates where the tourism \"item\" is located (e.g. restaurantX locatedIn Madrid)" ;
           rdfs:label "locatedIn" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#offersEvent
:offersEvent rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:AsymmetricProperty ;
             rdfs:domain :City ;
             rdfs:range :Event ;
             rdfs:comment "Connects a city to events held there" ;
             rdfs:label "offersEvent" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#recommendedRestaurantForEvent
:recommendedRestaurantForEvent rdf:type owl:ObjectProperty ,
                                        owl:AsymmetricProperty ;
                               rdfs:domain :Event ;
                               rdfs:range :Restaurant ;
                               rdfs:comment "Recommended restaurant for people that attends some event" ;
                               rdfs:label "recommendedRestaurantForEvent" .


#################################################################
#    Data properties
#################################################################

###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#happensIn
:happensIn rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Event ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "Date when an event takes place" ;
           rdfs:label "happensIn" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#hasPrice
:hasPrice rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Hotel
                                      :Restaurant
                                      :Transport
                                    )
                      ] ;
          rdfs:range xsd:decimal ;
          rdfs:comment "Indicates the price associated with the service (e.g price per night, meal, ticket, ...)" ;
          rdfs:label "hasPrice" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#hasRating
:hasRating rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Attraction
                                       :Hotel
                                       :Restaurant
                                     )
                       ] ;
           rdfs:range xsd:decimal ;
           rdfs:comment "Indicates the average rating for the service" ;
           rdfs:label "hasRating" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#hasStars
:hasStars rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Hotel ;
          rdfs:range [ rdf:type rdfs:Datatype ;
                       owl:onDatatype xsd:integer ;
                       owl:withRestrictions ( [ xsd:minInclusive 1
                                              ]
                                              [ xsd:maxInclusive 5
                                              ]
                                            )
                     ] ;
          rdfs:comment "Indicates the number of stars an acommodation has" ;
          rdfs:label "hasStars" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#runBy
:runBy rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf owl:topDataProperty ;
       rdf:type owl:FunctionalProperty ;
       rdfs:domain :Transport ;
       rdfs:range xsd:string ;
       rdfs:comment "Indicates the company that runs the service" ;
       rdfs:label "runBy" .


#################################################################
#    Classes
#################################################################

###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Andalusian
:Andalusian rdf:type owl:Class ;
            rdfs:subClassOf :Restaurant ;
            rdfs:comment "Restaurant serving andalusian food" ;
            rdfs:label "Andalusian Restaurant" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Attraction
:Attraction rdf:type owl:Class ;
            rdfs:subClassOf :TourismItem ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasRating ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                owl:onDatatype xsd:decimal ;
                                                owl:withRestrictions ( [ xsd:minInclusive 1.0
                                                                       ]
                                                                       [ xsd:maxInclusive 5.0
                                                                       ]
                                                                     )
                                              ]
                            ] ;
            rdfs:comment "Represents a place of interest for tourists, such as museums, parks, cultural attractions, etc." ;
            rdfs:label "Attraction" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Beach
:Beach rdf:type owl:Class ;
       rdfs:subClassOf :Attraction ;
       rdfs:comment "Type of attraction by the sea and/or near the beach" ;
       rdfs:label "Beach Attraction" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#BeachHotel
:BeachHotel rdf:type owl:Class ;
            rdfs:subClassOf :Hotel ;
            rdfs:comment "Accommodation near a beach with related services" ;
            rdfs:label "Beach Hotel" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Bus
:Bus rdf:type owl:Class ;
     rdfs:subClassOf :Transport ;
     rdfs:comment "Type of transport service using bus" ;
     rdfs:label "Bus" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Business
:Business rdf:type owl:Class ;
          rdfs:subClassOf :Hotel ;
          rdfs:comment "Business oriented accommodation with services as meeting rooms, etc" ;
          rdfs:label "Business Hotel" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#City
:City rdf:type owl:Class ;
      rdfs:comment "Geographic location where tourism resources are situated" ;
      rdfs:label "City" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Cultural
:Cultural rdf:type owl:Class ;
          rdfs:subClassOf :Event ;
          rdfs:comment "Event highlighting cultural traditions, art, etc." ;
          rdfs:label "Cultural Event" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#CulturalAttraction
:CulturalAttraction rdf:type owl:Class ;
                    rdfs:subClassOf :Attraction ;
                    rdfs:comment "Type of attraction providing access to the culture of the region" ;
                    rdfs:label "Cultural Attraction" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf :TourismItem ;
       rdfs:comment "Activities or shows of some interest for tourists" ;
       rdfs:label "Event" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Gastronomy
:Gastronomy rdf:type owl:Class ;
            rdfs:subClassOf :Event ;
            rdfs:comment "Event for food tasting and gastronomic culture in general" ;
            rdfs:label "Gastronomy Event" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#HighlyRatedAttraction
:HighlyRatedAttraction rdf:type owl:Class ;
                       rdfs:subClassOf :Attraction ;
                       rdfs:comment "Attractions with a rating of more than 3.0 points" ;
                       rdfs:label "Highly Rated Attraction" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Hotel
:Hotel rdf:type owl:Class ;
       rdfs:subClassOf :TourismItem ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasRating ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange [ rdf:type rdfs:Datatype ;
                                           owl:onDatatype xsd:decimal ;
                                           owl:withRestrictions ( [ xsd:minInclusive 1.0
                                                                  ]
                                                                  [ xsd:maxInclusive 5.0
                                                                  ]
                                                                )
                                         ]
                       ] ;
       rdfs:comment "Place of accomodation for tourists" ;
       rdfs:label "Hotel" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Italian
:Italian rdf:type owl:Class ;
         rdfs:subClassOf :Restaurant ;
         rdfs:comment "Restaurant serving italian food" ;
         rdfs:label "Italian Restaurant" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Japanese
:Japanese rdf:type owl:Class ;
          rdfs:subClassOf :Restaurant ;
          rdfs:comment "Restaurant serving japanese food" ;
          rdfs:label "Japanese Restaurant" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Luxury
:Luxury rdf:type owl:Class ;
        rdfs:subClassOf :Hotel ;
        rdfs:comment "Accommodation offering premium services and amenities" ;
        rdfs:label "Luxury Hotel" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Mediterranean
:Mediterranean rdf:type owl:Class ;
               rdfs:subClassOf :Restaurant ;
               rdfs:comment "Restaurant serving mediterranean food" ;
               rdfs:label "Mediterranean Restaurant" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Mexican
:Mexican rdf:type owl:Class ;
         rdfs:subClassOf :Restaurant ;
         rdfs:comment "Restaurant serving mexican food" ;
         rdfs:label "Mexican Restaurant" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Monument
:Monument rdf:type owl:Class ;
          rdfs:subClassOf :Attraction ;
          rdfs:comment "Type of attraction highlighting landmarks, historical events, etc." ;
          rdfs:label "Monument" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Museum
:Museum rdf:type owl:Class ;
        rdfs:subClassOf :Attraction ;
        rdfs:comment "Type of attraction displaying collections of artifacts, historical items, etc." ;
        rdfs:label "Museum" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Music
:Music rdf:type owl:Class ;
       rdfs:subClassOf :Event ;
       rdfs:comment "Type of event focusing on musical activities such as concerts of festivals" ;
       rdfs:label "Music Event" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Park
:Park rdf:type owl:Class ;
      rdfs:subClassOf :Attraction ;
      rdfs:comment "Type of attraction for outdoors recreation" ;
      rdfs:label "Park" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#PublicBycicle
:PublicBycicle rdf:type owl:Class ;
               rdfs:subClassOf :Transport ;
               rdfs:comment "Type of transport service using public bycicles" ;
               rdfs:label "Public Bycicle" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Restaurant
:Restaurant rdf:type owl:Class ;
            rdfs:subClassOf :TourismItem ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasRating ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                owl:onDatatype xsd:decimal ;
                                                owl:withRestrictions ( [ xsd:minInclusive 1.0
                                                                       ]
                                                                       [ xsd:maxInclusive 5.0
                                                                       ]
                                                                     )
                                              ]
                            ] ;
            rdfs:comment "Dinning service facilities" ;
            rdfs:label "Restaurant" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Rural
:Rural rdf:type owl:Class ;
       rdfs:subClassOf :Hotel ;
       rdfs:comment "Accommodation located in a rural area for visitors that seek a tranquil environment" ;
       rdfs:label "Rural Hotel" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Sports
:Sports rdf:type owl:Class ;
        rdfs:subClassOf :Event ;
        rdfs:comment "Type of event focusing on sports activities and competitions" ;
        rdfs:label "Sports Event" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Subway
:Subway rdf:type owl:Class ;
        rdfs:subClassOf :Transport ;
        rdfs:comment "Type of service transport using subway" ;
        rdfs:label "Subway" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Taxi
:Taxi rdf:type owl:Class ;
      rdfs:subClassOf :Transport ;
      rdfs:comment "Type of service transport using taxi" ;
      rdfs:label "Taxi" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Technology
:Technology rdf:type owl:Class ;
            rdfs:subClassOf :Event ;
            rdfs:comment "Type of event focusing on technology related activities" ;
            rdfs:label "Technology Event" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#TourismItem
:TourismItem rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :locatedIn ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :City
                             ] ;
             rdfs:comment "Any place, amenity or service with some touristic appealing" ;
             rdfs:label "Tourism Item" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Touristic
:Touristic rdf:type owl:Class ;
           rdfs:subClassOf :Hotel ;
           rdfs:comment "Accommodation for leisure travelers seeking comfort and convenience" ;
           rdfs:label "Touristic Hotel" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Tram
:Tram rdf:type owl:Class ;
      rdfs:subClassOf :Transport ;
      rdfs:comment "Type of service transport using tram" ;
      rdfs:label "Tram" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#Transport
:Transport rdf:type owl:Class ;
           rdfs:subClassOf :TourismItem ;
           rdfs:comment "Transportation options in a given place" ;
           rdfs:label "Transport" .


###  https://pvazquezr.github.io/REP_CONOC_PEC2/docs/ontology.owl#hotel_bargain
:hotel_bargain rdf:type owl:Class ;
               rdfs:subClassOf :Hotel ;
               rdfs:comment "Hotels with medium to low prices and very good reviews" ;
               rdfs:label "Bargain Accommodation" .


