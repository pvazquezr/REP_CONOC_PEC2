:Transport_Bus_Zaragoza_UrbanZaragoza rdf:type owl:NamedIndividual ,
                                               :Bus ;
                                      :locatedIn :City_Zaragoza ;
                                      :hasPrice 8.00 ;
                                      :runBy "Urban Zaragoza" ;
                                      rdfs:label "Urban Zaragoza Bus" .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Andalusian
                :Italian
                :Japanese
                :Mediterranean
                :Mexican
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Attraction
                :City
                :Event
                :Hotel
                :Restaurant
                :Transport
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Beach
                :Business
                :Luxury
                :Rural
                :Touristic
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Beach
                :CulturalAttraction
                :Monument
                :Museum
                :Park
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Bus
                :PublicBycicle
                :Subway
                :Taxi
                :Tram
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Cultural
                :Gastronomy
                :Music
                :Sports
                :Technology
              )
] .


#################################################################
#    Rules
#################################################################

<urn:swrl:var#r> rdf:type swrl:Variable .

<urn:swrl:var#a> rdf:type swrl:Variable .

<urn:swrl:var#e> rdf:type swrl:Variable .

<urn:swrl:var#rt> rdf:type swrl:Variable .

<urn:swrl:var#c> rdf:type swrl:Variable .

<urn:swrl:var#h> rdf:type swrl:Variable .

<urn:swrl:var#p> rdf:type swrl:Variable .

<urn:swrl:var#s> rdf:type swrl:Variable .

[ rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:BuiltinAtom ;
                           swrl:builtin swrlb:greaterThan ;
                           swrl:arguments [ rdf:type rdf:List ;
                                            rdf:first <urn:swrl:var#r> ;
                                            rdf:rest [ rdf:type rdf:List ;
                                                       rdf:first 3.0 ;
                                                       rdf:rest rdf:nil
                                                     ]
                                          ]
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:ClassAtom ;
                                      swrl:classPredicate :Attraction ;
                                      swrl:argument1 <urn:swrl:var#a>
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                                 swrl:propertyPredicate :hasRating ;
                                                 swrl:argument1 <urn:swrl:var#a> ;
                                                 swrl:argument2 <urn:swrl:var#r>
                                               ] ;
                                     rdf:rest rdf:nil
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate :HighlyRatedAttraction ;
