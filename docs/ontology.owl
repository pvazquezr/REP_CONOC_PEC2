    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Andalusian -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Andalusian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Restaurant"/>
        <rdfs:comment>Restaurant serving andalusian food</rdfs:comment>
        <rdfs:label>Andalusian Restaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Attraction -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Attraction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#TourismItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#hasRating"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</xsd:minInclusive>
                            </rdf:Description>
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">5.0</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:onDataRange>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Represents a place of interest for tourists, such as museums, parks, cultural attractions, etc.</rdfs:comment>
        <rdfs:label>Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Beach -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Beach">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Attraction"/>
        <rdfs:comment>Type of attraction by the sea and/or near the beach</rdfs:comment>
        <rdfs:label>Beach Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#BeachHotel -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#BeachHotel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Hotel"/>
        <rdfs:comment>Accommodation near a beach with related services</rdfs:comment>
        <rdfs:label>Beach Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Bus -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Bus">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Transport"/>
        <rdfs:comment>Type of transport service using bus</rdfs:comment>
        <rdfs:label>Bus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Business -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Business">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Hotel"/>
        <rdfs:comment>Business oriented accommodation with services as meeting rooms, etc</rdfs:comment>
        <rdfs:label>Business Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#City -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#City">
        <rdfs:comment>Geographic location where tourism resources are situated</rdfs:comment>
        <rdfs:label>City</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Cultural -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Cultural">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Event"/>
        <rdfs:comment>Event highlighting cultural traditions, art, etc.</rdfs:comment>
        <rdfs:label>Cultural Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#CulturalAttraction -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#CulturalAttraction">
