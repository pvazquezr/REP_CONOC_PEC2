    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Event -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Event">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#TourismItem"/>
        <rdfs:comment>Activities or shows of some interest for tourists</rdfs:comment>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


