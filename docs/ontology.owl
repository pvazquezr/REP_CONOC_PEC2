    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#HighlyRatedAttraction -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#HighlyRatedAttraction">
