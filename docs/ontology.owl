    </owl:Class>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Hotel -->


    <owl:Class rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Hotel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#TourismItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#hasRating"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</xsd:minInclusive>
                            </rdf:Description>
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">5.0</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:onDataRange>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Place of accomodation for tourists</rdfs:comment>
        <rdfs:label>Hotel</rdfs:label>
    </owl:Class>
    


