    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.1/created -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/created"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://www.w3.org/2002/07/owl#sameAs -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#locatedIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#locatedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#TourismItem"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#City"/>
        <rdfs:comment>Indicates where the tourism &quot;item&quot; is located (e.g. restaurantX locatedIn Madrid)</rdfs:comment>
        <rdfs:label>locatedIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#offersEvent -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#offersEvent">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#City"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Event"/>
        <rdfs:comment>Connects a city to events held there</rdfs:comment>
        <rdfs:label>offersEvent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#recommendedRestaurantForEvent -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#recommendedRestaurantForEvent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Event"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Restaurant"/>
        <rdfs:comment>Recommended restaurant for people that attends some event</rdfs:comment>
        <rdfs:label>recommendedRestaurantForEvent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#happensIn -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#happensIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Date when an event takes place</rdfs:comment>
        <rdfs:label>happensIn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#hasPrice -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#hasPrice">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Indicates the price associated with the service (e.g price per night, meal, ticket, ...)</rdfs:comment>
        <rdfs:label>hasPrice</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#hasRating -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#hasRating">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Indicates the average rating for the service</rdfs:comment>
        <rdfs:label>hasRating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#hasStars -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#hasStars">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Indicates the number of stars an acommodation has</rdfs:comment>
        <rdfs:label>hasStars</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#runBy -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#runBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Indicates the company that runs the service</rdfs:comment>
        <rdfs:label>runBy</rdfs:label>
    </owl:DatatypeProperty>
    


