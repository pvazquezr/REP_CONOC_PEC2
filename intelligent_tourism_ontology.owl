@prefix : <http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix intelligent_tourism_ontology: <http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#> .
@base <http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology/> .

<http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology> rdf:type owl:Ontology ;
                                                                                       <http://purl.org/dc/elements/1.1/created> "\"2025-04-27\"^^xsd:date" ;
                                                                                       <http://purl.org/dc/elements/1.1/creator> "Pablo Vázquez Rodríguez" ;
                                                                                       <http://purl.org/dc/elements/1.1/description> "In this ontology we model the domain of intelligent tourism, gathering information from different sources like events, hotels, attractions and restaurants in main touristic cities. The main goal is to provide recommendation systems that provide travelers useful informacion from a semantic structure able to combine geo data along user review values, events or activities." ;
                                                                                       <http://purl.org/dc/elements/1.1/title> "Recommendations and intelligent tourism ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/created
<http://purl.org/dc/elements/1.1/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#locatedIn
intelligent_tourism_ontology:locatedIn rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf owl:topObjectProperty ;
                                       rdf:type owl:FunctionalProperty ,
                                                owl:AsymmetricProperty ;
                                       rdfs:domain intelligent_tourism_ontology:TourismItem ;
                                       rdfs:range intelligent_tourism_ontology:City ;
                                       rdfs:comment "Indicates where the tourism \"item\" is located (e.g. restaurantX locatedIn Madrid)" ;
                                       rdfs:label "locatedIn" .


###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#offersEvent
intelligent_tourism_ontology:offersEvent rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf owl:topObjectProperty ;
                                         rdf:type owl:AsymmetricProperty ;
                                         rdfs:domain intelligent_tourism_ontology:City ;
                                         rdfs:range intelligent_tourism_ontology:Event ;
                                         rdfs:comment "Connects a city to events held there" ;
                                         rdfs:label "offersEvent" .


###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#recommendedRestaurantForEvent
intelligent_tourism_ontology:recommendedRestaurantForEvent rdf:type owl:ObjectProperty ,
                                                                    owl:AsymmetricProperty ;
                                                           rdfs:domain intelligent_tourism_ontology:Event ;
                                                           rdfs:range intelligent_tourism_ontology:Restaurant ;
                                                           rdfs:comment "Recommended restaurant for people that attends some event" ;
                                                           rdfs:label "recommendedRestaurantForEvent" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#happensIn
intelligent_tourism_ontology:happensIn rdf:type owl:DatatypeProperty ;
                                       rdfs:subPropertyOf owl:topDataProperty ;
                                       rdf:type owl:FunctionalProperty ;
                                       rdfs:range xsd:dateTime ;
                                       rdfs:comment "Date when an event takes place" ;
                                       rdfs:label "happensIn" .


###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#hasPrice
intelligent_tourism_ontology:hasPrice rdf:type owl:DatatypeProperty ;
                                      rdfs:subPropertyOf owl:topDataProperty ;
                                      rdf:type owl:FunctionalProperty ;
                                      rdfs:range xsd:decimal ;
                                      rdfs:comment "Indicates the price associated with the service (e.g price per night, meal, ticket, ...)" ;
                                      rdfs:label "hasPrice" .


###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#hasRating
intelligent_tourism_ontology:hasRating rdf:type owl:DatatypeProperty ;
                                       rdfs:subPropertyOf owl:topDataProperty ;
                                       rdf:type owl:FunctionalProperty ;
                                       rdfs:range xsd:decimal ;
                                       rdfs:comment "Indicates the average rating for the service" ;
                                       rdfs:label "hasRating" .


###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#hasStars
intelligent_tourism_ontology:hasStars rdf:type owl:DatatypeProperty ;
                                      rdfs:subPropertyOf owl:topDataProperty ;
                                      rdf:type owl:FunctionalProperty ;
                                      rdfs:range [ rdf:type rdfs:Datatype ;
                                                   owl:onDatatype xsd:integer ;
                                                   owl:withRestrictions ( [ xsd:minInclusive 1
                                                                          ]
                                                                          [ xsd:maxInclusive 5
                                                                          ]
                                                                        )
                                                 ] ;
                                      rdfs:comment "Indicates the number of stars an acommodation has" ;
                                      rdfs:label "hasStars" .


###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#runBy
intelligent_tourism_ontology:runBy rdf:type owl:DatatypeProperty ;
                                   rdfs:subPropertyOf owl:topDataProperty ;
                                   rdf:type owl:FunctionalProperty ;
                                   rdfs:range xsd:string ;
                                   rdfs:comment "Indicates the company that runs the service" ;
                                   rdfs:label "runBy" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Andalusian
intelligent_tourism_ontology:Andalusian rdf:type owl:Class ;
                                        rdfs:subClassOf intelligent_tourism_ontology:Restaurant ;
                                        rdfs:comment "Restaurant serving andalusian food" ;
                                        rdfs:label "Andalusian Restaurant" .


###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Attraction
intelligent_tourism_ontology:Attraction rdf:type owl:Class ;
                                        rdfs:subClassOf intelligent_tourism_ontology:TourismItem ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty intelligent_tourism_ontology:hasRating ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                                            owl:onDatatype xsd:decimal ;
                                                                            owl:withRestrictions ( [ xsd:minInclusive 1.0
                                                                                                   ]
                                                                                                   [ xsd:maxInclusive 5.0
                                                                                                   ]
                                                                                                 )
                                                                          ]
                                                        ] ;
                                        rdfs:comment "Represents a place of interest for tourists, such as museums, parks, cultural attractions, etc." ;
                                        rdfs:label "Attraction" .


###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#Beach
intelligent_tourism_ontology:Beach rdf:type owl:Class ;
                                   rdfs:subClassOf intelligent_tourism_ontology:Attraction ;
                                   rdfs:comment "Type of attraction by the sea and/or near the beach" ;
                                   rdfs:label "Beach Attraction" .


###  http://www.semanticweb.org/pablo_uoc/ontologies/2025/3/intelligent_tourism_ontology#BeachHotel
intelligent_tourism_ontology:BeachHotel rdf:type owl:Class ;
